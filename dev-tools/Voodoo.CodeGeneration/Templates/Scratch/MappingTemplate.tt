<#@ template language="C#"  linePragmas="false" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
 public class ScratchFile 
 {    			
        public  <#=File.Types.RightType.Name#> Map(<#=File.Types.LeftType.Name#> source, <#=File.Types.RightType.Name#> target )
        {
           <#foreach(var item in File.Types.ScalarProperties) {#>
					target.<#=item.Name#> = source.<#=item.Name#>;
		   <#}#>
		   return target;
        }
        
        public  <#=File.Types.LeftType.Name#> Map( <#=File.Types.RightType.Name#> source , <#=File.Types.LeftType.Name#> target)
        {
           <#foreach(var item in File.Types.ScalarProperties) {#>
					target.<#=item.Name#> =source.<#=item.Name#>;
		   <#}#>
		   return target;
        }
    }
}
